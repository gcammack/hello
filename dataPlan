 * A progarm to calculate how much Data on a phone plan has been use and how much is left
 *
 */
#include <stdlib.h>
#include <stdio.h>

int main(int argc, char **argv) {

  int dataAvailable, day, dataUsed;
  dataAvailable = argv[1];
  days = argv[2];
  dataUsed = argv[3];

  printf("%d days used, %d days remaining", days, 30 - days);

  return 0;
}

